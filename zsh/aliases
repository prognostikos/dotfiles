alias ls="ls -F"
alias vdiff="git diff|vimdiff -R -"
alias spr="tmux split-window -h -p 25; tmux send-keys 'simple_prompt; clear' C-m"
alias spd="tmux split-window -p 25; tmux send-keys 'simple_prompt; clear' C-m"
alias random_mac='sudo ifconfig en0 ether `openssl rand -hex 6 | sed "s/\(..\)/\1:/g; s/.$//"`'
alias restore_mac='sudo ifconfig en0 ether 60:03:08:aa:9d:9a'
alias vj='vim ~/d/journal.pgp'
